#!/usr/bin/env node
// eslint:disable

const fs = require('fs');
const path = require('path');
const project = path.join(__dirname, '../tsconfig.json');
const dev = fs.existsSync(project);
// const dev = null;

if (dev) {
  require('ts-node').register({ files: true });
}

require(`../${dev ? 'src' : 'lib'}`)
  .run()
  .catch(require('@oclif/errors/handle'));
